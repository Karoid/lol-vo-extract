{
	"env": {
		"node": true,
		"es2020": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"sourceType": "script"
	},
	"rules": {
		"indent": [ 0, "tab", { "ignoreComments": true } ],
		"linebreak-style": [ 2, "unix" ],
		"quotes": [ 0, "single" ],
		"semi": [ 2, "always" ],
		"no-unused-vars": [ 1, { "vars": "all", "args": "after-used" } ],
		"no-console": [ 2 ],
		"no-var": [ 2 ],
		"require-atomic-updates": [0]
	},
	"globals": {
		"L": true,
		"M": true,
		"T": true,
		"C": true,
		"P": true,
		"R": true,
		"RC": true,
		"RD": true,

		"_fs": true,
		"_pa": true,
		"_ul": true,
		"_as": true,
		"_cp": true,

		"BigInt": true,

		"Axios": true,
		"Fex": true,
		"Zstd": true,
		"Gzip": true,

		"Biffer": true
	}
}