{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "[Main] VO Extract",
			"runtimeExecutable": "node.exe",
			"program": "${workspaceFolder}/index.js",
			"skipFiles": [
				"<node_internals>/**"
			]
		},

		{
			"type": "node",
			"request": "launch",
			"name": "[Side] RSTConvert",
			"runtimeExecutable": "node.exe",
			"program": "${workspaceFolder}/side/rstc.js",
			"skipFiles": [
				"<node_internals>/**"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "[Side] Chromas",
			"runtimeExecutable": "node.exe",
			"program": "${workspaceFolder}/side/chromas.js",
			"skipFiles": [
				"<node_internals>/**"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "[Side] Find",
			"runtimeExecutable": "node.exe",
			// "console": "integratedTerminal",
			"program": "${workspaceFolder}/side/Find.js",
			"skipFiles": [
				"<node_internals>/**"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "bnk.js",
